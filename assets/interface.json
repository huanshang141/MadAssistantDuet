{
    "name": "MAD",
    "url": "https://github.com/huanshang141/MadAssistantDuet",
    "mirrorchyan_rid": "MadAssistantDuet",
    "mirrorchyan_multiplatform": true,
    "mfa_max_version": "v1.7.1",
    "mfa_min_version": "v1.7.1",
    "controller": [
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "二重螺旋|Duet Night Abyss",
                "input": 4,
                "screencap": 16
            }
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        }
    ],
    "agent": {
        "child_exec": "{PROJECT_DIR}/agent/MaaAgent.exe",
        "child_args": []
    },
    "task": [
        {
            "name":"闪避键设置（默认为L Shift）",
            "entry":"set_dodge_key",
            "doc":"设置闪避键，默认为L Shift",
            "option": ["闪避键设置"]
        },
        {
            "name": "自动战斗设置（默认为循环E）",
            "entry": "set_auto_battle_mode",
            "doc":"设置自动战斗的模式，默认是循环E，适用于水母等开E不会动的角色",
            "option": ["自动战斗设置"]
        },
        {
            "name": "65级mod（扼守）",
            "entry": "def_map1_entry",
            "doc": "进入指定副本后，即可启动，会先自动退出一次复位"
        },
        {
            "name": "驱离通用",
            "entry": "expulsion_entry",
            "doc": "驱离本的通用流程，进入副本后再启动脚本"
        },
        {
            "name": "60级皎皎币",
            "entry": "JJcoin_rounds",
            "doc": "60级皎皎币副本流程，进入副本后再启动脚本",
            "option": ["战斗轮数设置"]
        }
    ],
    "option": {
        "闪避键设置": {
            "cases": [
                {
                    "name": "左Shift键",
                    "pipeline_override": {
                        "set_dodge_key": {
                            "custom_action_param": {
                                "dodge_key": 160
                            }
                        }
                    }
                },
                {
                    "name": "Ctrl键",
                    "pipeline_override": {
                        "set_dodge_key": {
                            "custom_action_param": {
                                "dodge_key": 17
                            }
                        }
                    }
                },
                {
                    "name": "Alt键",
                    "pipeline_override": {
                        "set_dodge_key": {
                            "custom_action_param": {
                                "dodge_key": 18
                            }
                        }
                    }
                },
                {
                    "name": "空格键",
                    "pipeline_override": {
                        "set_dodge_key": {
                            "custom_action_param": {
                                "dodge_key": 32
                            }
                        }
                    }
                },
                {
                    "name": "数字5键",
                    "pipeline_override": {
                        "set_dodge_key": {
                            "custom_action_param": {
                                "dodge_key": 53
                            }
                        }
                    }
                },
                {
                    "name": "鼠标右键",
                    "pipeline_override": {
                        "set_dodge_key": {
                            "custom_action_param": {
                                "dodge_key": 2
                            }
                        }
                    }
                },
                {
                    "name": "鼠标侧键1",
                    "pipeline_override": {
                        "set_dodge_key": {
                            "custom_action_param": {
                                "dodge_key": 5
                            }
                        }
                    }
                },
                {
                    "name": "鼠标侧键2",
                    "pipeline_override": {
                        "set_dodge_key": {
                            "custom_action_param": {
                                "dodge_key": 6
                            }
                        }
                    }
                }
            ],
            "default_case": "左Shift键"
        },
        "自动战斗设置":{
            "cases": [
                {
                    "name": "循环E",
                    "pipeline_override": {
                        "set_auto_battle_mode":{
                            "custom_action_param":{
                                "auto_battle_mode": 0
                            }
                        }
                    }
                },
                                {
                    "name":"无操作",
                    "pipeline_override": {
                        "set_auto_battle_mode":{
                            "custom_action_param":{
                                "auto_battle_mode": 1
                            }
                        }
                    }
                }
            ],
            "default_case": "循环E"
        },
        "战斗轮数设置": {
            "cases": [
                {
                    "name": "1轮",
                    "pipeline_override": {
                        "JJcoin_rounds": {
                            "custom_action_param": {
                                "battle_rounds": 1
                            }
                        }
                    }
                },
                {
                    "name": "2轮",
                    "pipeline_override": {
                        "JJcoin_rounds": {
                            "custom_action_param": {
                                "battle_rounds": 2
                            }
                        }
                    }
                },
                {
                    "name": "3轮",
                    "pipeline_override": {
                        "JJcoin_rounds": {
                            "custom_action_param": {
                                "battle_rounds": 3
                            }
                        }
                    }
                },
                {
                    "name": "4轮",
                    "pipeline_override": {
                        "JJcoin_rounds": {
                            "custom_action_param": {
                                "battle_rounds": 4
                            }
                        }
                    }
                },
                {
                    "name": "5轮",
                    "pipeline_override": {
                        "JJcoin_rounds": {
                            "custom_action_param": {
                                "battle_rounds": 5
                            }
                        }
                    }
                },
                {
                    "name": "6轮",
                    "pipeline_override": {
                        "JJcoin_rounds": {
                            "custom_action_param": {
                                "battle_rounds": 6
                            }
                        }
                    }
                },
                {
                    "name": "7轮",
                    "pipeline_override": {
                        "JJcoin_rounds": {
                            "custom_action_param": {
                                "battle_rounds": 7
                            }
                        }
                    }
                },
                {
                    "name": "8轮",
                    "pipeline_override": {
                        "JJcoin_rounds": {
                            "custom_action_param": {
                                "battle_rounds": 8
                            }
                        }
                    }
                },
                {
                    "name": "9轮",
                    "pipeline_override": {
                        "JJcoin_rounds": {
                            "custom_action_param": {
                                "battle_rounds": 9
                            }
                        }
                    }
                },
                {
                    "name": "10轮",
                    "pipeline_override": {
                        "JJcoin_rounds": {
                            "custom_action_param": {
                                "battle_rounds": 10
                            }
                        }
                    }
                }
            ],
            "default_case": "3轮"
        }
    }
}
